{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "./",
  "modulePaths": ["../../"],
  "globalSetup": "../setup.ts",
  "testRegex": ".*\\.spec\\.ts$",
  "clearMocks": true,
  "restoreMocks": true,
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "../../../coverage",
  "testEnvironment": "node"
}
